<%= render 'layouts/innerpage'%>

<div class="container" style="margin-top: 100px">
	<h3>Friends from facebook</h3>
	<table class="table table-striped">
	  <tr>
	  	<th>Name</th>
	  </tr>
	  <% @friends.each do |f| %>
	  	<tr>	
	  	  <td><a href="http://facebook.com/<%= f["id"] %>"><%= f["name"] %></a></td>
	  	</tr>
	  <% end %>
	</table>
</div>